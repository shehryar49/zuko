# Simple Vector Object
# Implemented using very basic concepts
# of OOP
class Vector
{
  private var a = nil
  private var b = nil
  private var c = nil
  function __construct__(var a,var b,var c)
  {
     self.a = a
     self.b = b
     self.c = c
  }
  function add(var a)
  {
    self.a += a.a
    self.b += a.b
    self.c += a.c
  }
  function dotProduct(var a)
  {
    self.a *= a.a
    self.b *= a.b
    self.c *= a.c
  }
  function display()
  {
    println(self.a,"i + ",self.b,"j + ",self.c,"k")
  }

}

var v1 = Vector(2,3,5)
var v2 = Vector(3,2,0)
v1.dotProduct(v2)
v1.display()
