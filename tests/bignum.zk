import bignum

# test bignum module

fn fact(var n)
{
    var k = bignum.bignum("1")
    var ans = bignum.bignum("1")
    while(k<=n)
    {
        ans*=k
        k.increment()
    }
    return ans
}
println(fact(bignum.bignum("10")).strval() == "3628800")

var a = bignum.bignum("5")
var b = bignum.bignum("2")

println((a-b).strval() == "3")
println((b-a).strval() == "-3")