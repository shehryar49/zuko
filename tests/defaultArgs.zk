fn square(var x = 30)
{
    return x*x
}
fn length(var list = [1,2,3])
{
    return len(list)
}
fn lshift(var x,var n=1)
{
    return x<<n
}
fn sum(var x,var y=40,var z=29)
{
    return x+y+z
}
println(square(3))
println(square())
println(length([1,2]))
println(length())
println(lshift(1))
println(lshift(1,4)) # 16
println(sum(3)) #72
println(sum(3,5)) #37
println(sum(3,5,9)) # 17


