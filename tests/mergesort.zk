fn merge(var arr,var l,var mid,var h)
{
    var arr1 = []
    var arr2 = []
    for(var i=l to mid step 1)
      arr1.push(arr[i])
    for(var i=mid+1 to h step 1)
      arr2.push(arr[i])
    var i = 0
    var j = 0
    var k = l
    while(i<len(arr1) and j<len(arr2))
    {
        if(arr1[i]<= arr2[j])
        {
            arr[k] = arr1[i]
            k+=1
            i+=1
        }
        else
        {
            arr[k] = arr2[j]
            j+=1
            k+=1
        }
    }
    while(i<len(arr1))
    {
        arr[k] = arr1[i]
        k+=1
        i+=1
    }
    while(j<len(arr2))
    {
        arr[k] = arr2[j]
        k+=1
        j+=1
    }
    
}
fn mergeSort(var arr,var l,var h)
{
    if(l<h)
    {
        var mid = (l+h)/2
        mergeSort(arr,l,mid)
        mergeSort(arr,mid+1,h)
        merge(arr,l,mid,h)
    }
}
var arr = [-5,4,3,2,1]
mergeSort(arr,0,4)
