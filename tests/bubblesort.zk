
function bubbleSort(var arr)
{
  var n = len(arr)
  var i = 1
  while(i<=n-1)
  {
    var j = 0
    while(j<=n-2)
    {
      if(arr[j] > arr[j+1])
      {
        var c = arr[j]
        arr[j] = arr[j+1]
        arr[j+1] = c
      }
      j+=1
    }
    i+=1
  }
}
var arr = [10,-40,0,4,4,7, 8, 9, 1, 5]
println(arr)
bubbleSort(arr)
println(arr)
